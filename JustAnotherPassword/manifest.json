{
  "manifest_version": 3,
  "name": "JustAnotherPassword",
  "version": "3.0.0",
  "description": "Generate random passwords with random numbers and base64 encoding.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "host_permissions": []
}
